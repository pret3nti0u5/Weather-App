{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "func-names": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "no-undef": "warn",
        "no-use-before-define": ["error", {"functions": false, "classes": false}]
    },
    "globals": {
        "document": false,
        "fetch": false
    }
}